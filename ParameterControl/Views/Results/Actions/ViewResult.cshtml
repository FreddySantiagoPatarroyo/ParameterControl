@using ParameterControl.Models.Result;
@model Result;

@{
}

<partial name="_Menu" />

<div class="box_view">
    <div class="bg__blur"></div>
    <form class="form__container">

        <div class="card__container">
            <div class="header_form">
                <img src="/images/iconApple.png" />
                <h1>Detalles del Resultado</h1>
            </div>

            <div class="form_group">
                <p class="title_form">Conciliación:</p>
                <p>@Model.Conciliation</p>
            </div>
            <div class="form_groupm">
                <p class="title_form">Escenario:</p>
                <p>@Model.Scenery</p>
            </div>
            <div class="form_group">
                <p class="title_form">Estado:</p>
                <p>@Model.Status</p>
            </div>
            <div class="form_group">
                <p class="title_form">Fecha Inicio:</p>
                <p>@Model.StartDate</p>
            </div>
            <div class="form_groupm">
                <p class="title_form">Fecha Fin:</p>
                <p>@Model.EndDate</p>
            </div>
            <div class="form_group">
                <p class="title_form">Valor Bene:</p>
                <p>@Model.BeneValue</p>
            </div>
            <div class="form_group">
                <p class="title_form">Valor Inco:</p>
                <p>@Model.IncoValue</p>
            </div>
            <div class="form_groupm">
                <p class="title_form">Valor PQ:</p>
                <p>@Model.PQValue</p>
            </div>
            <div class="form_group">
                <p class="title_form">Valor Rein:</p>
                <p>@Model.ReinValue</p>
            </div>

            <div class="btn_container">
                <a class="button button_secondary btn_cancel-view" asp-controller="Results" asp-action="Results"><p class="text font_bold">Cancelar</p></a>
            </div>
        </div>
    </form>
</div>