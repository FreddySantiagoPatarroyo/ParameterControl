@using ParameterControl.Models.Result;
@model Result;

@{
    ViewData["Title"] = "Activar Resultados";
}

<partial name="_Menu" />

<div class="box_delete">
    <div class="bg__blur"></div>
    <form class="form__container">

        <div class="card__container">
            <div class="header_form">
                <img src="/images/iconApple.png" />
                <h1>Activar Resultado</h1>
                <h2>¿Está seguro de activar el resultado @Model.Conciliation?</h2>
            </div>

            <div class="btn_container">
                <a class="button button_secondary" asp-controller="Results" asp-action="Results"><p class="text font_bold">Cancelar</p></a>
                <button class="button button_primary"><p class="text font_bold">Activar</p></button>
            </div>
        </div>
    </form>
</div>
