@using ParameterControl.Models.Parameter
@using ParameterControl.Models.Pagination
@model PaginationResult<TableParametersViewModel>

@{
    ViewData["Title"] = "Parametros";
    string ErrorSuccess = "Se genero un problema al obtener la informacion,Home";
}

<partial name="_Menu" model="@ViewBag.InfoUser" />

@if (!ViewBag.Success)
{
    <partial name="_ModalError" model="ErrorSuccess" />
}
else
{
    <partial name="_TableParameters" model="Model" />
}

@section Scripts {
}